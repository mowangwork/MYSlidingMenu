<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mywork.myslidingmenu" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".general.LeftMenuActivity"
            android:label="@string/title_activity_left_menu" >
        </activity>
        <activity
            android:name=".general.RightMenuActivity"
            android:label="@string/title_activity_right_menu" >
        </activity>
        <activity
            android:name=".general.LeftAndRightActivity"
            android:label="@string/title_activity_left_and_right" >
        </activity>
        <activity
            android:name=".dynamic.LeftDynamicMenuActivity"
            android:label="@string/title_activity_left_dynamic_menu" >
        </activity>
        <activity
            android:name=".dynamic.RightDynamicMenuActivity"
            android:label="@string/title_activity_right_dynamic_menu" >
        </activity>
        <activity
            android:name=".dynamic.LeftAndRightDynamicActivity"
            android:label="@string/title_activity_left_and_right_dynamic" >
        </activity>
        <activity
            android:name=".effect.LockInterfaceActivity"
            android:label="@string/title_activity_lock_interface" >
        </activity>
        <activity
            android:name=".effect.LockInterfaceDynamicActivity"
            android:label="@string/title_activity_lock_interface_dynamic" >
        </activity>
    </application>

</manifest>
